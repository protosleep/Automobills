<Window x:Class="AutomobillsWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutomobillsWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Bottom" MinWidth="150" >
            <Button Content="Overview Page" MinWidth="150" Click="OverviewPage_Click" Margin="2,0,0,0"/>
            <Button Content="Budget" MinWidth="150" Margin="2,0,0,0" Click="Budget_Click"/>
        </StackPanel>
        <Frame x:Name="PageFrame" Margin="0,0,0,25" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
